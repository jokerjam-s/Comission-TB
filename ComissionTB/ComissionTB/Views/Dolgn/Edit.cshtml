@model ComissionTB.Data.Dolgn

@{
    ViewData["Title"] = "Изменить должность";
}

<h1>Изменить должность</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="id_dl" />
            <div class="form-group">
                <label asp-for="dlName" class="control-label"></label>
                <input asp-for="dlName" class="form-control" />
                <span asp-validation-for="dlName" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Изменить" class="btn btn-warning" />
                <a asp-action="Index" class="btn btn-primary">К списку</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
